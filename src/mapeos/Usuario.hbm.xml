<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mvc.bean">
    <class name="Usuario" table="usuario">
        <id name="usuario" column="usuario"></id>
        <property name="nombre"  column="nombre" />
        <property name="clave" column="clave"/>
        <property name="telefono" column="telefono"/>
        <property name="direccion"  column="direccion" />
        <property name="correo" column="correo"/>

    </class>
</hibernate-mapping>
